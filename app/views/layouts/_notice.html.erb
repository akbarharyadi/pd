<% if data != nil %>
      <div class="alert alert-info">
          <button type="button" class="close" data-dismiss="alert">
            <i class="ace-icon fa fa-times"></i>
          </button>
          <strong>
            <i class="ace-icon fa fa-bell"></i>
            <%= raw(data) %>
          </strong>
      </div>
    <% end %>