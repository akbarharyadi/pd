
<% unless smart_listing.empty? %>
<div class="table-container">
  <table class="table table-striped table-bordered">
    <thead>
      <tr role="row">
        <th class="sorting">nomor Setor</th>
        <th class="sorting">No Pendataan</th>
        <th class="sorting">Rekening</th>
        <th class="sorting">NPWPD</th>
        <th class="sorting">Nama Usaha</th>
        <th class="sorting">Periode</th>
        <th class="sorting">Tanggal Setor</th>
        <th></th>
      </tr>
    </thead>

    <tbody>
      <% 
          counting = 1
      %>
      <% smart_listing.collection.each do |penetapan| %>
              <% if counting.odd? %>
                  <tr role="row" class="odd">
              <% else %>
                <tr role="row" class="even">
              <% end %>
          <td><%= penetapan.try(:no_setor) %></td>
          <td><%= penetapan.pendataan.no_pendataan %></td>
          <td><%= penetapan.pendataan.rekening.nama_rekening %></td>
          <td><%= penetapan.pendataan.pendaftaran.npwpd %></td>
          <td><%= penetapan.pendataan.pendaftaran.nama_usaha %></td>
          <td><%= I18n.l(penetapan.pendataan.periode_awal, format:'%d %B %Y') + ' s/d ' + I18n.l(penetapan.pendataan.periode_akhir, format:'%d %B %Y') %></td>
          <td><%= I18n.l(penetapan.tgl_setor, format:'%d %B %Y') if penetapan.try(:no_setor).present? %></td>
          <td>
              <div class="action-buttons">
                <%= link_to(raw('<i class="ace-icon fa fa-print bigger-130"></i>'), penyetoran_proses_path(id: penetapan.id, tanggal:nil, format: 'pdf')) %>
              </div>
          </td>
        </tr>
        <% counting += 1 %>
      <% end %>
    </tbody>
  </table>
</div>
<%= smart_listing.paginate  %>
<% else %>
<p> Data tidak ditemukan </p>
<% end %>