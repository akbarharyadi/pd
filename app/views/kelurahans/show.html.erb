<div class="row">
  <div class="col-xs-12">
    <div class="widget-box">
      <div class="widget-header widget-header-flat">
        <h4 class="widget-title smaller">Detail Kelurahan</h4>
        <div class="widget-toolbar">
          <%= link_to(raw('<i class="ace-icon fa fa-pencil align-center"></i> Edit'), edit_kecamatan_path(@kelurahan)) %> | 
          <%= link_to(raw('<i class="ace-icon fa fa-undo icon-only"></i> kembali'), kelurahans_path) %> |
          <%= link_to(raw('<i class="ace-icon fa fa-trash icon-only"></i> Hapus'), @kelurahan, method: :delete, data: { confirm: 'Ada yakin ingin menghapus data ini?' }) %>
        </div>
      </div>
      <div class="widget-body">
        <div class="widget-main">
          <dl id="dt-list-1" class="">
            <dt><strong>Kode Kelurahan:</strong></dt>
            <dd><%= @kelurahan.kode %></dd>
            <dt><strong>Nama Kelurahan:</strong></dt>
            <dd><%= @kelurahan.nama %></dd>
					</dl>
        </div>
      </div>
    </div>
  </div>
</div>



