<% if error_form.errors.any? %>
      <div class="alert alert-danger">
          <button type="button" class="close" data-dismiss="alert">
            <i class="ace-icon fa fa-times"></i>
          </button>
          <strong>
            <i class="ace-icon fa fa-times"></i>
            <%= pluralize(error_form.errors.count, "error") %> :
          </strong>

        <ul>
        <% error_form.errors.full_messages.each do |message| %>
          <li><%= message %></li>
        <% end %>
        </ul>
      </div>
    <% end %>